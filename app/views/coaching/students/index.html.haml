- provide(:page_title, "Students")
- provide(:page_header, "Students")

.page-card
  .page-card-header
    = link_to "Add New", new_coaching_student_path, class: "btn btn-primary m-0", data: { turbo_frame: "remote_modal_form" }, title: "Add student"

  .page-card-body
    - if @students.present?
      = render "table"
      = render "shared/pagy"
    - else
      = render "shared/empty_record"
